<Window x:Class="TimeDeal.Infrom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="Infrom" Height="300" Width="300"  Foreground="{x:Null}" WindowStyle="None" MouseDown="Window_MouseDown">
    <Grid Height="272" Width="291" Background="{x:Null}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100*" />
            <ColumnDefinition Width="189" />
        </Grid.ColumnDefinitions>
        <Label Height="27" HorizontalAlignment="Stretch" Margin="100,30,62,0" Name="label_Title" VerticalAlignment="Top" Width="129" Grid.ColumnSpan="2" FontSize="16" FontFamily="Microsoft Yi Baiti" Content="起床" FontStretch="Normal" FontWeight="Bold"/>
        <TextBox Height="149" HorizontalAlignment="Left" Margin="31,63,0,0" Name="textBox_Content" VerticalAlignment="Top" Width="217" Grid.ColumnSpan="2" Background="{x:Null}" IsReadOnly="True" TextWrapping="Wrap" BorderBrush="{x:Null}" FontSize="16" />
        <Button Content="OK" Grid.Column="1" Height="30" HorizontalAlignment="Left" Margin="64,218,0,0" VerticalAlignment="Top" Width="63" Name="Button_OK" Background="{x:Null}" Click="Button_OK_Click"></Button>
    </Grid>
    <Window.Background>
        <ImageBrush ImageSource="/TimeDeal;component/Images/Inform01.gif" />
    </Window.Background>
</Window>
